type
  Pos = { x: Num, y: Num }
  Rev = { y: Num, x: Num }


def swap_coords(p: Pos) -> None =
  var x = p.x
  p.x = p.y
  p.y = x


var pos = Pos{ 5, 6 }
var rev = Rev{ 6, 5 }
pos
rev

swap_coords(pos)
swap_coords(rev)
pos
rev
